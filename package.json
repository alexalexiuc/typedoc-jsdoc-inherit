{
  "name": "typedoc-jsdoc-inherit",
  "version": "0.0.1",
  "description": "A Typedoc plugin to propagate JSDoc tags to re-exported reflections.",
  "main": "dist-esm/index.mjs",
  "type": "module",
  "exports": {
    "import": "./dist/index.mjs"
  },
  "scripts": {
    "build": "node ./esbuild.js",
    "test": "jest",
    "prepare": "husky",
    "lint:check": "eslint \"**/*.ts\"",
    "lint:fix": "eslint \"**/*.ts\" --fix",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run lint:check && npm run type-check && npm test",
    "lerna:version": "lerna version --exact --conventional-commits --conventional-graduate --no-push --no-git-tag-version --no-commit-hooks --loglevel silent --yes",
    "lint-staged": "lint-staged"
  },
  "keywords": [
    "typedoc",
    "plugin",
    "jsdoc",
    "tags",
    "typescript"
  ],
  "author": "Alexiuc Alex",
  "license": "MIT",
  "dependencies": {
    "typedoc": "^0.27.4",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.16.0",
    "@release-it/conventional-changelog": "^9.0.3",
    "@swc/core": "^1.10.1",
    "@swc/jest": "^0.2.37",
    "@types/jest": "^29.5.14",
    "esbuild": "^0.24.0",
    "eslint": "^9.16.0",
    "eslint-plugin-jest": "^28.9.0",
    "husky": "^9.1.7",
    "jest": "^29.0.0",
    "lerna": "^8.1.9",
    "lint-staged": "^15.2.11",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.18.0"
  },
  "release-it": {
    "git": {
      "commitMessage": "Release v${version} [skip ci]",
      "tagName": "release/v${version}"
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "infile": "CHANGELOG.md",
        "header": "# Changelog",
        "preset": {
          "name": "conventionalcommits",
          "types": [
            {
              "type": "feat",
              "section": ":sparkles: New Features"
            },
            {
              "type": "fix",
              "section": ":bug: Bug Fixes"
            },
            {
              "type": "test",
              "hidden": true
            },
            {
              "type": "chore",
              "hidden": true
            },
            {
              "type": "refactor",
              "hidden": true
            },
            {
              "type": "perf",
              "hidden": true
            },
            {
              "type": "docs",
              "hidden": true
            }
          ],
          "commitUrlFormat": "{{host}}/{{owner}}/{{repository}}/commits/{{hash}}",
          "compareUrlFormat": "{{host}}/{{owner}}/{{repository}}/branches/compare/{{currentTag}}%0D{{previousTag}}#diff"
        }
      }
    }
  },
  "lint-staged": {
    "packages/**/src/**/*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
